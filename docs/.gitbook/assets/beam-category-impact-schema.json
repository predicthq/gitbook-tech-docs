  "$id": "https://docs.predicthq.com/docs/schema/beam-category-impact-schema.json",
  "type": "object",
  "required": [
    "count",
    "impact",
    "percentage"
  ],
  "properties": {
    "count": {
      "type": "integer",
      "minimum": 0
    },
    "impact": {
      "type": "number",
      "minimum": 0
    },
    "percentage": {
      "type": "number",
      "minimum": 0
    }
  },
  "additionalProperties": false
}
